    public void limpiar(){
        this.tf_codigo.setText("");
        this.tf_run.setText("");
        this.tf_nombre.setText("");
        this.tf_apellido.setText("");
        this.tf_celular.setText("");
        this.tf_email.setText("");
        this.tf_sueldo.setText("");
        this.cb_estado_civil.setSelectedItem("Seleccione");
        this.cb_depto.setSelectedItem("Seleccione");
        this.tf_codigo.requestFocus();
    }
